A,KAAA,IAAA,CAAA,MARA;AASA,gBAAA,IAAA,EAAA,KAAA,IAAA,CAAA,MATA;AAUA,gBAAA,KAAA,EAAA,KAAA,IAAA,CAAA;AAVA,eAHA;AAeA,cAAA,OAAA,EAAA,iBAAA,MAAA,EAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAIA,oBAAA,MAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,WAAA;AACA;AACA;AAvBA,aAAA;AAyBA;AAEA;AAGA;AACA,KA1EA;;AA2EA;;;AAGA,IAAA,IA9EA,kBA8EA;AAAA;;AACA,WAAA,QAAA,CAAA,eAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA;AACA,OANA,EAMA,KANA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAXA;AAYA,KA3FA;AA4FA,IAAA,KA5FA,iBA4FA,CA5FA,EA4FA;AACA,WAAA,QAAA,CAAA;AACA,QAAA,OAAA,EAAA,CADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAAA;AAIA;AAjGA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-row>\r\n      <el-col :span=\"24\">\r\n        <el-card>\r\n          <div slot=\"header\" class=\"clearfix\" style=\"text-align:center\">\r\n            <span>健康信息上报</span>\r\n          </div>\r\n          <el-table\r\n            :data=\"tableData\"\r\n            border\r\n            style=\"width: 100%;\">\r\n            <el-table-column\r\n              prop=\"username\"\r\n              label=\"姓名\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"classes\"\r\n              label=\"班级\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"address\"\r\n              label=\"生源地\">\r\n            </el-table-column>\r\n          </el-table>\r\n        </el-card>\r\n        <el-divider></el-divider>\r\n\r\n      <el-card v-if=\"show==false\">\r\n          <div slot=\"header\" style=\"text-align: center\">\r\n              <span>今日检测情况</span>\r\n          </div>\r\n          <el-alert\r\n                  title=\"今日申请表提交成功\"\r\n                  type=\"success\"\r\n                  center\r\n                  show-icon>\r\n          </el-alert>\r\n          <el-table\r\n                  :data=\"tableData1\"\r\n                  border\r\n                  style=\"width: 100%\">\r\n              <el-table-column\r\n                      prop=\"temperature\"\r\n                      label=\"当前体温\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"hot\"\r\n                      label=\"是否发热、咳嗽\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"fever\"\r\n                      label=\"是否有去就诊\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"gohubei\"\r\n                      label=\"是否去过高危地区\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"hubeiren\"\r\n                      label=\"是否为高危地区人员\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"mask\"\r\n                      label=\"是否接触过高危地区人员\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"leaveout\"\r\n                      label=\"是否离开过学校\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"hesuan\"\r\n                      label=\"是否做过核酸检测\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"masknum\"\r\n                      label=\"口罩数量\">\r\n              </el-table-column>\r\n              <el-table-column\r\n                      prop=\"kills\"\r\n                      label=\"杀毒用品是否充足\">\r\n              </el-table-column>\r\n          </el-table>\r\n\r\n      </el-card>\r\n\r\n        <el-card v-else-if=\"show==true\">\r\n          <el-form ref=\"form\" :model=\"form\" label-width=\"150px\">\r\n                <el-form-item label=\"当前体温\">\r\n                    <el-input v-model=\"form.temp\" style=\"width:130px\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否发热、咳嗽\">\r\n                    <el-radio-group v-model=\"form.radio1\">\r\n                    <el-radio label=\"否\"></el-radio>\r\n                    <el-radio label=\"是\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否有去就诊\">\r\n                    <el-radio-group v-model=\"form.radio2\">\r\n                    <el-radio label=\"否\"></el-radio>\r\n                    <el-radio label=\"是\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否去过高危地区\">\r\n                    <el-radio-group v-model=\"form.radio3\">\r\n                    <el-radio label=\"否\"></el-radio>\r\n                    <el-radio label=\"是\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n              <el-form-item label=\"是否为高危地区人员\">\r\n                  <el-radio-group v-model=\"form.radio9\">\r\n                      <el-radio label=\"否\"></el-radio>\r\n                      <el-radio label=\"是\"></el-radio>\r\n                  </el-radio-group>\r\n              </el-form-item>\r\n                <el-form-item label=\"是否接触过高危地区人员\">\r\n                    <el-radio-group v-model=\"form.radio7\">\r\n                    <el-radio label=\"否\"></el-radio>\r\n                    <el-radio label=\"是\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否离开过学校\">\r\n                    <el-radio-group v-model=\"form.radio5\">\r\n                    <el-radio label=\"否\"></el-radio>\r\n                    <el-radio label=\"是\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"是否做过核酸检测\">\r\n                    <el-radio-group v-model=\"form.radio6\">\r\n                    <el-radio label=\"否\"></el-radio>\r\n                    <el-radio label=\"是\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"口罩数量\">\r\n                    <el-input v-model=\"form.number\" style=\"width:130px\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"杀毒用品是否充足\">\r\n                    <el-radio-group v-model=\"form.radio8\">\r\n                    <el-radio label=\"否\"></el-radio>\r\n                    <el-radio label=\"是\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" @click=\"open\">提交</el-button>\r\n                </el-form-item>\r\n                </el-form>\r\n        </el-card>\r\n\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n      return {\r\n          show:true,\r\n        tableData: [],\r\n          tableData1: [],\r\n        form: {\r\n          temp: '',\r\n          number:'',\r\n          radio1: '',\r\n          radio2: '',\r\n          radio3: '',\r\n          radio4: '',\r\n          radio5: '',\r\n          radio6: '',\r\n          radio7: '',\r\n          radio8: '',\r\n            radio9: '',\r\n        }\r\n      }\r\n    },\r\n    created(){\r\n        /**根据token获取当前用户信息\r\n         * this.tableData列表数据\r\n         * */\r\n        this.$axiosjwt({\r\n            url:\"/users/getUserDataByToken\",\r\n            method:\"get\",\r\n            data:{},\r\n            success:(result)=>{\r\n                this.tableData.push(result)\r\n            }\r\n        })\r\n\r\n        /**\r\n         *\r\n         *调用获取当天某用户报表并且决定是否允许填写\r\n         */\r\n        this.getnowdatah()\r\n    },\r\n     methods: {\r\n         /**\r\n          *   //获取当天某用户报表并且决定是否允许填写\r\n          */\r\n      getnowdatah(){\r\n          this.$axiosjwt({\r\n              url:'/students/getHealthNowDayByid',\r\n              method:\"get\",\r\n              data:{},\r\n              success:(result)=> {\r\n                //   console.log(result,\"xxxxxxxxxxxxx这是结果!!!!!!!!!!!!!!!!!!!!!---------------------\")\r\n                  if(result.length==0) this.show=true\r\n                  else {\r\n                      this.show=false;\r\n                      this.tableData1.push(result[0])\r\n                  }\r\n              }\r\n          });\r\n      },/**\r\n          健康表提交\r\n          temperature:this.form.temp,\r\n          hot:this.form.radio1,\r\n          gohubei:this.form.radio3,\r\n          hubeiren:this.form.radio9,\r\n          leave:this.form.radio5,\r\n          hesuan:this.form.radio6,\r\n          mask:this.form.radio7,\r\n          masknum:this.form.number,\r\n          kill:this.form.radio8,\r\n          fever:this.form.radio2,\r\n          */\r\n         onSubmit() {\r\n             if (!this.form.temp || !this.form.radio1 || !this.form.radio3 || !this.form.radio9 || !this.form.radio5 || !this.form.radio6 ||!this.form.radio7 ||!this.form.number || !this.form.radio8 || !this.form.radio2)\r\n                this.open3(\"填报表不可留空\")\r\n             else{\r\n                 let reg=/^\\d+(\\.\\d+)?$/\r\n                 let reg1=/^(([0-9]+\\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\\.[0-9]+)|([0-9]*[1-9][0-9]*))$/\r\n                 let tempBool=reg1.test(this.form.temp)\r\n                 let   mskBool=reg.test(this.form.number)\r\n                 if (!tempBool) this.open3(\"体温请输入数字\")\r\n                 else{\r\n                     if (!mskBool)  this.open3(\"口罩数量请用数字方式输入\")\r\n                     else{\r\n                         this.$axiosjwt({\r\n                             url:\"/students/sethealth\",\r\n                             method:\"post\",\r\n                             data:{\r\n                                 temperature:this.form.temp,\r\n                                 hot:this.form.radio1,\r\n                                 gohubei:this.form.radio3,\r\n                                 hubeiren:this.form.radio9,\r\n                                 leave:this.form.radio5,\r\n                                 hesuan:this.form.radio6,\r\n                                 mask:this.form.radio7,\r\n                                 masknum:this.form.number,\r\n                                 kill:this.form.radio8,\r\n                                 fever:this.form.radio2,\r\n                             },\r\n                             success:(result)=>{\r\n                                 this.$message({\r\n                                     type:'success',\r\n                                     message:\"提交成功\"\r\n                                 })\r\n                                 if (result.length!=0){\r\n                                     this.getnowdatah()\r\n                                 }\r\n                             }\r\n                         })\r\n                     }\r\n                     \r\n                 }\r\n\r\n\r\n             }\r\n         },\r\n         /**\r\n          * 提交弹窗\r\n          */\r\n         open() {\r\n             this.$confirm('提交健康报表, 是否继续?', '提示', {\r\n                 confirmButtonText: '确定',\r\n                 cancelButtonText: '取消',\r\n                 type: 'warning'\r\n             }).then(() => {\r\n                 this.onSubmit()\r\n             }).catch(() => {\r\n                 this.$message({\r\n                     type: 'info',\r\n                     message: '已取消提交'\r\n                 });\r\n             });\r\n         },\r\n         open3(v) {\r\n             this.$message({\r\n                 message:v,\r\n                 type: 'warning'\r\n             });\r\n         },\r\n\r\n    },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/main"}]}