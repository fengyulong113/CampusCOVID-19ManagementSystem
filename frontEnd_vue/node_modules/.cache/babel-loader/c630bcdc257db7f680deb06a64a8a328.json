;AAMA,OAPA;AAQA,KAbA;;AAeA;;;AAGA,IAAA,SAlBA,uBAkBA;AACA,UAAA,KAAA,SAAA,KAAA,YAAA,EAAA;AACA,aAAA,YAAA;AACA,OAFA,MAEA;AACA,YAAA,CAAA,KAAA,YAAA,EAAA,KAAA,QAAA,CAAA,OAAA,CAAA,WAAA,EAAA,KACA,KAAA,YAAA;AACA;AACA,KAzBA;;AA2BA;;;AAGA,IAAA,YA9BA,0BA8BA;AAAA;;AACA,MAAA,yBAAA,CAAA;AACA,QAAA,SAAA,EAAA,KAAA,SADA;AAEA,QAAA,YAAA,EAAA,KAAA;AAFA,OAAA,CAAA,CAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA;AACA,iDACA,IADA;AAEA,YAAA,IAAA,EAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AAFA;AAIA,SALA,CAAA;AAMA,OAXA,EAYA,KAZA,CAYA,UAAA,GAAA;AAAA,eAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AAAA,OAZA;AAaA,KA5CA;;AA8CA;;;AAGA,IAAA,YAjDA,0BAiDA;AAAA;;AACA,MAAA,yBAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA;AACA,iDACA,IADA;AAEA,YAAA,IAAA,EAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AAFA;AAIA,SALA,CAAA;AAMA,OAPA,EAOA,KAPA,CAOA,UAAA,GAAA;AAAA,eAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AAAA,OAPA;AAQA,KA1DA;;AA4DA;;;AAGA,IAAA,WA/DA,yBA+DA;AACA,WAAA,iBAAA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,KAnEA;;AAoEA;;;AAGA,IAAA,gBAvEA,4BAuEA,CAvEA,EAuEA,GAvEA,EAuEA;AAAA;;AACA,MAAA,oBAAA,CAAA;AACA,QAAA,IAAA,EAAA,GAAA,CAAA;AADA,OAAA,CAAA,CAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA;AACA,iDACA,IADA;AAEA,YAAA,UAAA,EAAA,UAAA,CAAA,IAAA,CAAA,UAAA;AAFA;AAIA,SALA,CAAA;AAMA,QAAA,MAAA,CAAA,aAAA,GAAA,IAAA;AACA,OAVA;AAWA,KAnFA;AAoFA;AACA,IAAA,WArFA,yBAqFA;AACA,WAAA,aAAA,GAAA,KAAA;AACA;AAvFA;AAdA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-row>\r\n      <el-col>\r\n        <el-card style=\"margin-bottom: 10px\">\r\n          <div slot=\"header\" class=\"clearfix\" style=\"text-align: left\">\r\n            <span>查询学生健康信息</span>\r\n          </div>\r\n          <el-select\r\n            v-model=\"queryType\"\r\n            placeholder=\"请选择查询类型\"\r\n            style=\"width: 20%; margin-right: 10px\"\r\n          >\r\n            <el-option label=\"班级\" value=\"u_classes\"></el-option>\r\n            <el-option label=\"学号\" value=\"u_id\"></el-option>\r\n            <el-option label=\"姓名\" value=\"u_name\"></el-option>\r\n            <el-option label=\"体温高于37.2℃\" value=\"q_tempHigh\"></el-option>\r\n          </el-select>\r\n          <el-input\r\n            placeholder=\"请输入内容\"\r\n            prefix-icon=\"el-icon-search\"\r\n            v-model=\"queryContent\"\r\n            style=\"width: 20%\"\r\n          >\r\n          </el-input>\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"small\"\r\n            style=\"margin-left: 10px\"\r\n            @click=\"searchStu\"\r\n            >搜索</el-button\r\n          >\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"small\"\r\n            style=\"margin-left: 10px\"\r\n            @click=\"resetValuse\"\r\n            >重置</el-button\r\n          >\r\n        </el-card>\r\n        <el-card v-if=\"!stuHealthShow\">\r\n          <div slot=\"header\" class=\"clearfix\" style=\"text-align: center\">\r\n            <span>学生健康管理</span>\r\n          </div>\r\n          <el-table :data=\"stuList\" border style=\"width: 100%\">\r\n            <el-table-column prop=\"u_id\" label=\"学号\"> </el-table-column>\r\n            <el-table-column prop=\"u_classes\" label=\"班级\"> </el-table-column>\r\n            <el-table-column prop=\"u_name\" label=\"姓名\"> </el-table-column>\r\n            <el-table-column prop=\"temperature\" label=\"今日体温\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"PCR\" label=\"今日核酸是否为阴性\">\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\">\r\n              <template slot-scope=\"scope\">\r\n                <el-button\r\n                  style=\"padding: 3px 0\"\r\n                  type=\"text\"\r\n                  @click=\"selectUserHealth(scope.$index, scope.row)\"\r\n                >\r\n                  查看健康信息\r\n                </el-button>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n        </el-card>\r\n        <el-card v-else-if=\"stuHealthShow\">\r\n          <div slot=\"header\" class=\"clearfix\" style=\"text-align: center\">\r\n            <span>{{stuHealthData[0].u_name}}的个人健康填报信息</span>\r\n            <el-button\r\n              style=\"float: right; padding: 3px 0\"\r\n              type=\"text\"\r\n              @click=\"closeHealth\"\r\n              >关闭</el-button\r\n            >\r\n          </div>\r\n          <el-table :data=\"stuHealthData\" border style=\"width: 100%\">\r\n            <el-table-column prop=\"createtime\" label=\"填报时间\"> </el-table-column>\r\n            <el-table-column prop=\"temperature\" label=\"体温\"> </el-table-column>\r\n            <el-table-column prop=\"isHot\" label=\"是否发热\"> </el-table-column>\r\n            <el-table-column prop=\"PCR\" label=\"核酸检测\"> </el-table-column>\r\n          </el-table>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getHealthNowDayRequest,\r\n  getHealthByIdRequest,\r\n  getStuHealthByTypeRequest,\r\n  getStuHealthByTempRequest\r\n} from \"@/api/request\";\r\nimport { formatID, formatTime } from '@/api/utils';\r\nexport default {\r\n  name: \"stuHealth\",\r\n  data() {\r\n    return {\r\n      stuList: [], //今日学生健康信息\r\n      queryType: \"\", //查询类型（班级/学号/体温）\r\n      queryContent: \"\", //查询内容\r\n      stuHealthShow: false,\r\n      stuHealthData: [], //学生个人健康信息\r\n    };\r\n  },\r\n  created() {\r\n    this.getAllHealthToday()\r\n  },\r\n  methods: {\r\n    /**\r\n     * 获取今日所有已填报健康信息的学生信息\r\n     */\r\n    getAllHealthToday() {\r\n      getHealthNowDayRequest().then((res) => {\r\n        this.stuList = res.map((item) => {\r\n          return {\r\n            ...item,\r\n            u_id: formatID(item.u_id),\r\n          };\r\n        });\r\n      });\r\n    },\r\n\r\n    /**\r\n     * 查询\r\n     */\r\n    searchStu() {\r\n      if(this.queryType === 'q_tempHigh'){\r\n        this.searchByTemp()\r\n      }else{\r\n        if(!this.queryContent) this.$message.warning(\"请输入要搜索的内容\")\r\n        else this.searchByType()\r\n      }\r\n    },\r\n\r\n    /**\r\n     * 通过类型查询\r\n     */\r\n    searchByType(){\r\n      getStuHealthByTypeRequest({\r\n        queryType: this.queryType,\r\n        queryContent: this.queryContent,\r\n      })\r\n        .then((res) => {\r\n          this.stuList = res.map((item) => {\r\n            return {\r\n              ...item,\r\n              u_id: formatID(item.u_id),\r\n            };\r\n          });\r\n        })\r\n        .catch((err) => this.$message.error(err));\r\n    },\r\n\r\n    /**\r\n     * 通过体温查询\r\n     */\r\n    searchByTemp(){\r\n      getStuHealthByTempRequest().then(res => {\r\n        this.stuList = res.map((item) => {\r\n            return {\r\n              ...item,\r\n              u_id: formatID(item.u_id),\r\n            };\r\n          });\r\n      }).catch(err => this.$message.error(err))\r\n    },\r\n\r\n    /**\r\n     * 重置\r\n     */\r\n    resetValuse() {\r\n      this.getAllHealthToday()\r\n      this.queryType = '';\r\n      this.queryContent = '';\r\n    },\r\n    /**\r\n     * 查看学生个人的所有健康信息\r\n     */\r\n    selectUserHealth(_, row) {\r\n      getHealthByIdRequest({\r\n        u_id: row.u_id,\r\n      }).then((res) => {\r\n        this.stuHealthData = res.map(item => {\r\n          return {\r\n            ...item,\r\n            createtime: formatTime(item.createtime)\r\n          }\r\n        });\r\n        this.stuHealthShow = true;\r\n      });\r\n    },\r\n    //关闭学生个人的所有健康信息\r\n    closeHealth() {\r\n      this.stuHealthShow = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/components/teacher"}]}