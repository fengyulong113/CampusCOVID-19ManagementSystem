ames":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiIA;AACA;AACA","file":"Teacher.vue","sourceRoot":"src/components/teacher","sourcesContent":["<template>\r\n  <div class=\"common\">\r\n\r\n    <nav class=\"main-header navbar navbar-expand navbar-white navbar-light\">\r\n      <ul class=\"navbar-nav\">\r\n        <li class=\"nav-item\">\r\n          <a class=\"nav-link\" data-widget=\"pushmenu\" href=\"#\" role=\"button\">\r\n            <i class=\"fas fa-bars\"></i>\r\n          </a>\r\n        </li>\r\n        <li class=\"nav-item d-none d-sm-inline-block\">\r\n          <router-link tag=\"a\" to=\"/teacher/home\" class=\"nav-link\"\r\n            >Home</router-link\r\n          >\r\n        </li>\r\n      </ul>\r\n      <ul class=\"navbar-nav ml-auto\">\r\n        <span>Campus COVID-19 Management System</span>\r\n      </ul>\r\n      <ul class=\"navbar-nav ml-auto\">\r\n        <el-dropdown>\r\n          <span class=\"el-dropdown-link\">\r\n            <a class=\"nav-link\" data-widget=\"pushmenu\" href=\"#\" role=\"button\">\r\n              <i class=\"el-icon-s-tools\"></i>\r\n            </a>\r\n          </span>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item>\r\n              <i class=\"el-icon-switch-button\"></i>\r\n              <el-button type=\"text\" @click=\"outlogin\">退出登录</el-button>\r\n            </el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n        <div class=\"dropdown-divider\"></div>\r\n      </ul>\r\n    </nav>\r\n\r\n    <aside class=\"main-sidebar sidebar-dark-primary elevation-4\">\r\n      <span class=\"font-weight-light brand-link\">{{ type }}</span>\r\n      <div class=\"sidebar\">\r\n        <div class=\"user-panel mt-3 pb-3 mb-3 d-flex\">\r\n          <div class=\"image\">\r\n            <img\r\n              :src=\"avatar\"\r\n              class=\"img-circle elevation-2\"\r\n              alt=\"User Image\"\r\n              style=\"width: 3rem; height: 3rem\"\r\n            />\r\n          </div>\r\n          <div class=\"info\">\r\n            <a href=\"#/teacher/user\" class=\"d-block\">{{username}}</a>\r\n          </div>\r\n        </div>\r\n        <nav class=\"mt-2\">\r\n          <ul\r\n            class=\"nav nav-pills nav-sidebar flex-column\"\r\n            data-widget=\"treeview\"\r\n            role=\"menu\"\r\n            data-accordion=\"false\"\r\n          >\r\n            <router-link\r\n              tag=\"li\"\r\n              v-for=\"(item, index) in navActive\"\r\n              :key=\"index\"\r\n              class=\"nav-item\"\r\n              :to=\"item.path\"\r\n            >\r\n              <a class=\"nav-link\">\r\n                <i :class=\"item.icon\"></i>\r\n                <p>\r\n                  {{ item.title }}\r\n                </p>\r\n              </a>\r\n            </router-link>\r\n          </ul>\r\n        </nav>\r\n      </div>\r\n    </aside>\r\n\r\n    <div class=\"content-wrapper\">\r\n      <section class=\"content\" style=\"height: 48rem; overflow-y: scroll\">\r\n        <div class=\"container-fluid\">\r\n          <el-row style=\"padding: 10px 10px\">\r\n            <el-col :span=\"24\">\r\n              <transition name=\"el-fade-in-linear\" mode=\"out-in\">\r\n                <router-view></router-view>\r\n              </transition>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getUserDataByToken } from '@/api/request';\r\nimport { baseURL } from \"@/api/config\";\r\nimport { teacherRouterAdmin } from '@/api/utils';\r\nexport default {\r\n  name: \"Admin\",\r\n  data() {\r\n    return {\r\n      type: \"\",\r\n      avatar: \"\",\r\n      username: \"\",\r\n      navActive: teacherRouterAdmin\r\n    };\r\n  },\r\n  created() {\r\n    getUserDataByToken()\r\n      .then((res) => {\r\n        this.username = res.username;\r\n        this.avatar = baseURL + \"/file/\" + res.head;\r\n    if (res.type == 2) this.type = \"Student\";\r\n        else if (res.type == 3) this.type = \"Teacher\";\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  },\r\n  methods: {\r\n    outlogin() {\r\n      window.location.href = \"http://localhost:80/#/Login\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.content::-webkit-scrollbar {\r\n  display: none;\r\n}\r\n</style>\r\n"]}]}