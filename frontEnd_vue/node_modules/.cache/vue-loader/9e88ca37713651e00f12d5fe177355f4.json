;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmXA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA","file":"noticeSend.vue","sourceRoot":"src/components/main","sourcesContent":["<template>\r\n    <div>\r\n        <el-row :gutter=\"20\">\r\n            <el-col :span=\"24\">\r\n                <el-card shadow=\"hover\">\r\n                    <div slot=\"header\" style=\"text-align: center\">\r\n                        <span>填写通知</span>\r\n                    </div>\r\n                    <div style=\"display: flex;justify-content: center;margin-right: 10px;\">\r\n                        <!--<span style=\"margin: auto 0\">主题</span>-->\r\n                        <div style=\"margin: auto 0\"><el-input v-model=\"input\" placeholder=\"主题\"></el-input></div>\r\n                        <!--<span style=\"margin: auto 0\">班级</span>-->\r\n                        <div style=\"margin: auto 0\">\r\n                            <el-select v-model=\"value\" multiple placeholder=\"班级（多选）\">\r\n                                <el-option\r\n                                        v-for=\"item in options\"\r\n                                        :key=\"item.c_id\"\r\n                                        :label=\"item.classes\"\r\n                                        :value=\"item.classes\">\r\n                                </el-option>\r\n                            </el-select>\r\n                        </div>\r\n                        <div style=\"margin: auto 0\"><el-button type=\"primary\" @click=\"noticeSend\">发布</el-button></div>\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n\r\n        </el-row>\r\n        <el-divider><i class=\"el-icon-loading\"></i></el-divider>\r\n        <el-row :gutter=\"20\">\r\n            <el-col :span=\"24\">\r\n                <el-card v-if=\"!show\" shadow=\"hover\">\r\n                    <div slot=\"header\" class=\"clearfix\" style=\"text-align: center\">\r\n                        <span>通知详情查看</span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"cls\">关闭</el-button>\r\n                    </div>\r\n                    <el-table\r\n                            :data=\"tableData1\"\r\n                            border\r\n                            style=\"width: 100%\">\r\n                        <el-table-column\r\n                                prop=\"n_id\"\r\n                                label=\"序列号\">\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                                prop=\"title\"\r\n                                label=\"主题\">\r\n                        </el-table-column>\r\n                        <!--<el-table-column-->\r\n                        <!--prop=\"class\"-->\r\n                        <!--label=\"通知的班级\">-->\r\n                        <!--</el-table-column>-->\r\n                        <el-table-column\r\n                                label=\"发布时间\">\r\n                            <template slot-scope=\"scope\">{{ scope.row.createtime | date}}</template>\r\n                        </el-table-column>\r\n                    </el-table>\r\n                    <el-row :gutter=\"20\" style=\"margin-top: 10px\">\r\n                        <el-col :span=\"8\">\r\n                            <el-card style=\"height: 300px\" shadow=\"hover\">\r\n                                <ve-pie :data=\"chartData\"\r\n                                        :settings=\"Settings\"\r\n                                        :legend-visible=\"false\">\r\n                                </ve-pie>\r\n                            </el-card>\r\n                        </el-col>\r\n                        <el-col :span=\"16\">\r\n                            <el-card shadow=\"hover\">\r\n                                <div slot=\"header\" class=\"clearfix\" style=\"text-align: center\">\r\n                                    <span>访问的用户</span>\r\n                                </div>\r\n                                <el-row :gutter=\"20\"style=\"overflow-y: scroll;height: 12.5rem\">\r\n\r\n\r\n                                    <el-col :span=\"4\"  v-for=\"(item,index) in users\" :key=\"index\" class=\"ca\" style=\"margin-bottom: 3rem;margin-top: 0.7rem\"  >\r\n                                        <el-tooltip placement=\"top\">\r\n                                            <div slot=\"content\">阅读时间：{{ item.readtime | date}}</div>\r\n                                            <el-card shadow=\"hover\" :body-style=\"{ padding: '0px' }\" style=\"min-height: 11rem\" class=\"imgcar\">\r\n                                                <div style=\"width:100%;height:6rem ;\">\r\n                                                    <img :src=\"imgPath+item.head\" style=\"width:100%;height:100%  ; object-fit:cover;\" >\r\n                                                </div>\r\n                                                <div style=\"margin-top: 0.8rem;padding-left: 0.8rem; padding-right: 0.8rem;margin-bottom: 1rem ; height:3rem;margin-bottom: 5px; overflow-y: scroll\" >\r\n                                                    <span>{{item.username}}</span>\r\n                                                    <div class=\"bottom clearfix\" style=\" height: 1rem;overflow: hidden\">\r\n                                                        <time class=\"time\">{{item.classes}}</time>\r\n                                                    </div>\r\n                                                </div>\r\n                                            </el-card>\r\n                                        </el-tooltip>\r\n                                    </el-col>\r\n                                </el-row>\r\n                            </el-card>\r\n                        </el-col>\r\n                    </el-row>\r\n                </el-card>\r\n                <el-card v-else shadow=\"hover\">\r\n                    <el-table\r\n                            :data=\"tableData\"\r\n                            border\r\n                            :default-sort = \"{prop: 'n_id', order: 'descending'}\"\r\n                            style=\"width: 100%\">\r\n                        <el-table-column\r\n                                prop=\"n_id\"\r\n                                sortable\r\n                                label=\"序列号\">\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                                prop=\"title\"\r\n                                label=\"主题\">\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                                label=\"创建时间\">\r\n                            <template slot-scope=\"scope\">{{ scope.row.createtime | date}}</template>\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                                label=\"操作\">\r\n                            <template slot-scope=\"scope\">\r\n                                <el-button style=\"padding: 3px 0\" type=\"text\" @click=\"open(scope.$index, scope.row)\">删除</el-button>\r\n                                <el-button style=\"padding: 3px 0\" type=\"text\" @click=\"selectshow(scope.$index, scope.row)\" >查看详情</el-button>\r\n                            </template>\r\n                        </el-table-column>\r\n                    </el-table>\r\n                    <div class=\"block\" style=\"text-align:center;margin-top:20px\">\r\n                        <el-pagination\r\n                                @size-change=\"handleSizeChange\"\r\n                                @current-change=\"handleCurrentChange\"\r\n                                :current-page=\"currentPage\"\r\n                                :page-sizes=\"[10, 20, 30, 40,50]\"\r\n                                :page-size=\"10\"\r\n                                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                                :total=\"UserSize\">\r\n                        </el-pagination>\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n        </el-row>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        data() {\r\n            return {\r\n                Settings: {\r\n                    offsetY: 120,\r\n                    radius: 50,\r\n                    // level: [],\r\n                    itemStyle: {\r\n                        center: ['20%', '10%']\r\n                    },\r\n                    label: {\r\n                        normal: {\r\n                            fontSize: 16,\r\n                            formatter: '{b}' + '\\n\\r' + '{c}' + '\\n\\r' + '({d}%)'\r\n                        }\r\n                    }\r\n                },\r\n                imgPath:this.basePath+\"/file/\",\r\n                show:true,\r\n                input:'',\r\n                UserSize:0,\r\n                currentPage: 1,\r\n                tableData: [],\r\n                tableData1: [],\r\n                pageSize:10,\r\n                pageNo:1,\r\n                value: '',\r\n                //先写死数据\r\n                options: [{\"classes\":\"1班\"},{\"classes\":\"2班\"},{\"classes\":\"3班\"}],\r\n                users:[],\r\n                readtime:[],\r\n                chartData: {\r\n                    columns: ['类型', '用户'],\r\n                    rows: []\r\n                }\r\n            }\r\n        },\r\n        filters:{\r\n            date(time){\r\n                var dt = new Date(time);\r\n                // yyyy-mm-dd\r\n                var y = dt.getFullYear();\r\n                var m = dt.getMonth() + 1;\r\n                var d = dt.getDate();\r\n                var hh = dt.getHours();\r\n                var mm = dt.getMinutes();\r\n                var ss = dt.getSeconds();\r\n                return y + \"-\" + m + \"-\" + d + \"  \" + hh + \":\" + mm + \":\" + ss\r\n            },\r\n        },\r\n        created(){\r\n            /**\r\n             * 调用获取所有通知\r\n             */\r\n            this.getvalue(this.pageSize,this.pageNo)\r\n            /**\r\n             * 获取班级表(增值功能)\r\n             */\r\n            this.$axiosjwt({\r\n                url:\"/admin/getClasses\",\r\n                method:\"get\",\r\n                data:{},\r\n                success:(result)=>{\r\n                    this.options=result\r\n                }\r\n            })\r\n\r\n        },\r\n        filters:{\r\n            date(time){\r\n                var dt = new Date(time);\r\n                // yyyy-mm-dd\r\n                var y = dt.getFullYear();\r\n                var m = dt.getMonth() + 1;\r\n                var d = dt.getDate();\r\n                var hh = dt.getHours();\r\n                var mm = dt.getMinutes();\r\n                var ss = dt.getSeconds();\r\n                return y + \"-\" + m + \"-\" + d + \"  \" + hh + \":\" + mm + \":\" + ss\r\n            }\r\n        },\r\n        methods:{\r\n            //关闭\r\n            cls(){\r\n                let arr=[];\r\n                this.chartData.rows=arr\r\n                this.show=true;\r\n            },\r\n\r\n            open2(message) {\r\n                this.$message({\r\n                    message: message||'恭喜你，这是一条成功消息',\r\n                    type: 'success'\r\n                });\r\n            },\r\n            open3(v) {\r\n                this.$message({\r\n                    message: v,\r\n                    type: 'warning'\r\n                });\r\n            },\r\n            open(index, row) {\r\n                this.$confirm('此操作将继续, 是否继续?', '提示', {\r\n                    confirmButtonText: '确定',\r\n                    cancelButtonText: '取消',\r\n                    type: 'warning'\r\n                }).then(() => {\r\n                    this.delNotice(index,row);\r\n                    this.$message({\r\n                        type: 'success',\r\n                        message: '删除成功!'\r\n                    });\r\n                }).catch(() => {\r\n                    this.$message({\r\n                        type: 'info',\r\n                        message: '已取消删除'\r\n                    });\r\n                });\r\n            },\r\n            /**\r\n             * 查看通知详情(增值功能)\r\n             n_id:row.n_id,\r\n             * */\r\n            selectshow(index,row){\r\n                this.$axiosjwt({\r\n                    url:\"/admin/NoticeDetails\",\r\n                    method:\"get\",\r\n                    data:{\r\n                        n_id:row.n_id,\r\n                    },\r\n                    success:(result)=> {\r\n                        let arr =[];\r\n                        this.tableData1 = result.data\r\n                        this.users=result.users;\r\n                        this.readtime=result.idAndtime\r\n                        this.chartData.rows.push(\r\n                            { '类型': '已读', '用户': result.readNum },\r\n                            { '类型': '未读', '用户': result.total-result.readNum },\r\n                        )\r\n                        this.show=false;\r\n                    }\r\n                });\r\n            },\r\n            /**\r\n             * 删除通知(增值功能,后期实现)\r\n             n_id:row.n_id,\r\n             this.pageSize,this.pageNo\r\n             * */\r\n            delNotice(index,row){\r\n                this.$axiosjwt({\r\n                    url:\"/admin/delNotice\",\r\n                    method:\"get\",\r\n                    data:{\r\n                        n_id:row.n_id,\r\n                    },\r\n                    success:(result)=>{\r\n                        this.getvalue(this.pageSize,this.pageNo)\r\n                    }\r\n                })\r\n            },\r\n\r\n            /**\r\n             * 分页获取所有通知\r\n             * @param pageNum\r\n             * @param currPage\r\n             *\r\n             pageNum:pageNum,\r\n             currPage:currPage-1\r\n             this.tableData=result.data;\r\n             this.UserSize = result.total\r\n             */\r\n            getvalue(pageNum,currPage){\r\n                this.$axiosjwt({\r\n                    url:'/admin/getAllNotice',\r\n                    method:'get',\r\n                    data:{\r\n                        pageNum:pageNum,\r\n                        currPage:currPage-1\r\n                    },\r\n                    success:(result)=> {\r\n                        this.tableData=result.data;\r\n                        this.UserSize = result.total\r\n                    }\r\n                });\r\n            },\r\n            /**\r\n             * 发布通知\r\n             *   title:this.input,\r\n                 classes:s,\r\n             */\r\n            noticeSend(){\r\n                if (this.value.length==0|| !this.input){\r\n                    this.open3(\"输入不可为空,请输入内容再发布\")\r\n                }else{\r\n                    let s=this.value.join(\";\")\r\n                    this.$axiosjwt({\r\n                        url:\"/admin/announce\",\r\n                        method:\"post\",\r\n                        data:{\r\n                            title:this.input,\r\n                            classes:s,\r\n                        },success:(result)=>{\r\n                            this.input=\"\"\r\n                            this.value=\"\"\r\n                            this.open2(result)\r\n                            this.getvalue(this.pageSize,this.pageNo)\r\n                        }\r\n                    })\r\n                }\r\n\r\n\r\n\r\n\r\n            },\r\n\r\n\r\n\r\n            //分页\r\n            handleSizeChange(val) {\r\n                this.pageSize=val;\r\n                this.getvalue(this.pageSize,this.pageNo)\r\n            },\r\n            handleCurrentChange(val) {\r\n                this.pageNo=val;\r\n                this.getvalue(this.pageSize,this.pageNo)\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n    ::-webkit-scrollbar{\r\n        display: none\r\n    }\r\n    /* .ca{\r\n         height:15rem ;\r\n     }*/\r\n    .imgcar{\r\n        min-height:11rem;\r\n        display: flex;\r\n        flex-flow: column;\r\n        align-items: center;\r\n        /*margin-top: 2rem;*/\r\n\r\n    }\r\n    .time {\r\n        font-size: 13px;\r\n        color: #999;\r\n    }\r\n\r\n    .bottom {\r\n        margin-top: 13px;\r\n        line-height: 12px;\r\n    }\r\n\r\n    .button {\r\n        padding: 0;\r\n        float: right;\r\n    }\r\n\r\n    .image {\r\n        width: 40px;\r\n        height: 40px;\r\n        display: block;\r\n    }\r\n\r\n    .clearfix:before,\r\n    .clearfix:after {\r\n        display: table;\r\n        content: \"\";\r\n    }\r\n\r\n    .clearfix:after {\r\n        clear: both\r\n    }\r\n</style>"]}]}